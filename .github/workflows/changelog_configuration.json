{
  "categories": [
      {
          "title": "## Enhancements",
          "labels": ["kind/feature"]
      },
      {
          "title": "## Bug Fixes",
          "labels": ["fix"]
      }
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}}   - PR: #${{NUMBER}}",
  "empty_template": "- no changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1"
    },
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "on_property": "title"
    }
  ],
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "tag_resolver": {
    "method": "semver"
  }  
}